{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "problemMatcher": "$msCompile",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "--nologo",
        "${workspaceFolder}",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "test",
      "problemMatcher": "$msCompile",
      "command": "dotnet",
      "type": "process",
      "args": [
        "test",
        "--nologo",
        "${workspaceFolder}",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "group": {
        "kind": "test",
        "isDefault": true
      }
    },
    {
      "label": "format",
      "problemMatcher": "$msCompile",
      "command": "dotnet",
      "type": "process",
      "args": [
        "format",
        "${workspaceFolder}"
      ]
    },
    {
      "label": "check",
      "problemMatcher": "$msCompile",
      "command": "dotnet",
      "type": "process",
      "args": [
        "format",
        "--check",
        "${workspaceFolder}"
      ]
    },
    {
      "label": "package",
      "problemMatcher": "$msCompile",
      "command": "dotnet",
      "type": "process",
      "args": [
        "pack",
        "${workspaceFolder}",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ]
    }
  ]
}